Algoritmo Ejercicio_1_promedio_de_edad
	Escribir "INGRESE SU EDAD PARTICIPANTE 1";
	Leer EDAD1;
	Escribir "INGRESE SU EDAD PARTICIPANTE 2";
	Leer EDAD2;
	Escribir "INGRESE SU EDAD PARTICIPANTE 3"
